[
  {
    "battery": {
      "bat": "BAT0",
      "format": "{icon} {capacity}% {time}",
      "format-charging": "{icon} {capacity}%({time} {power:.0f}W)",
      "format-full": "100%",
      "format-icons": [
        "󰁺",
        "󰁻",
        "󰁼",
        "󰁽",
        "󰁾",
        "󰁿",
        "󰂀",
        "󰂁",
        "󰂂",
        "󰁹"
      ],
      "format-time": "{H}:{m}",
      "full-at": 97,
      "interval": 5,
      "states": {
        "critical": 15,
        "warning": 30
      },
      "tooltip": false
    },
    "clock#date": {
      "locale": "en_GB.UTF-8",
      "calendar": {
        "format": {
          "days": "<span color='#ecc6d9'><b>{}</b></span>",
          "months": "<span color='#ffead3'><b>{}</b></span>",
          "today": "<span color='#ff6699'><b><u>{}</u></b></span>",
          "weekdays": "<span color='#ffcc66'><b>{}</b></span>",
          "weeks": "<span color='#99ffdd'><b>W{:%V}</b></span>"
        },
        "mode": "month",
        "mode-mon-col": "3",
        "on-click-right": "mode",
        "on-scroll": 1,
        "weeks-pos": "left"
      },
      "format": "{:%Y-%m-%d-%a}",
      "interval": 10,
      "tooltip-format": "<tt><small>{calendar}</small></tt>"
    },
    "clock#time": {
      "locale": "en_GB.UTF-8",
      "calendar": {
        "format": {
          "days": "<span color='#ecc6d9'><b>{}</b></span>",
          "months": "<span color='#ffead3'><b>{}</b></span>",
          "today": "<span color='#ff6699'><b><u>{}</u></b></span>",
          "weekdays": "<span color='#ffcc66'><b>{}</b></span>",
          "weeks": "<span color='#99ffdd'><b>W{:%V}</b></span>"
        },
        "mode": "month",
        "mode-mon-col": "3",
        "on-click-right": "mode",
        "on-scroll": 1,
        "weeks-pos": "left"
      },
      "format": "{:%I:%M:%S %p}",
      "interval": 1,
      "tooltip-format": "<tt><small>{calendar}</small></tt>"
    },
    "cpu": {
      "format": " {usage:2}%",
      "interval": 2,
      "tooltip": false
    },
    "custom/keyboard": {
      "exec": "{{ swaywm_waybar_scripts_dir }}/toggle_keyboard.py --state",
      "format": "{} ",
      "interval": 1,
      "on-click": "{{ swaywm_waybar_scripts_dir }}/toggle_keyboard.py",
      "return-type": "json",
      "tooltip": false
    },
    "custom/separator": {
      "format": "|",
      "tooltip": false
    },
    "disk": {
      "format": " {specific_used:0.0f}|{specific_total:0.0f}({percentage_used}%)",
      "interval": 10,
      "path": "/",
      "tooltip": false,
      "unit": "GB"
    },
    "idle_inhibitor": {
      "format": "{icon}",
      "format-icons": {
        "activated": "<span color='#ff3300'><u> </u></span>",
        "deactivated": " "
      },
      "tooltip": false
    },
    "layer": "top",
    "memory": {
      "format": " {used:0.0f}|{total:0.0f}",
      "interval": 2,
      "tooltip": false
    },
    "mode": "dock",
    "modules-center": [],
    "modules-left": [
      "sway/workspaces"
    ],
    "modules-right": [
      "network",
      "pulseaudio",
      "idle_inhibitor",
      "custom/keyboard",
      "cpu",
      "memory",
      "disk",
      "temperature",
      "battery",
      "clock#date",
      "clock#time",
      "tray"
    ],
    "mpd": {
      "consume-icons": {
        "on": ""
      },
      "format": "{stateIcon} {consumeIcon}{randomIcon}{repeatIcon}{singleIcon}{albumArtist} - {title}",
      "format-disconnected": "Disconnected ",
      "format-stopped": "{consumeIcon}{randomIcon}{repeatIcon}{singleIcon}Stopped ",
      "interval": 2,
      "random-icons": {
        "off": "<span color=\"#f53c3c\"></span> ",
        "on": " "
      },
      "repeat-icons": {
        "on": " "
      },
      "single-icons": {
        "on": "1 "
      },
      "state-icons": {
        "paused": "",
        "playing": ""
      },
      "tooltip-format": "MPD (connected)",
      "tooltip-format-disconnected": "MPD (disconnected)",
      "unknown-tag": "N/A"
    },
    "network": {
      "format": "{ifname}",
      "format-disconnected": "not connected",
      "format-ethernet": "󰈀 ({ifname}): {ipaddr}",
      "format-wifi": "󰖩 {essid} {ipaddr}/{cidr} ({signalStrength}%)",
      "interval": 3,
      "tooltip": false
    },
    "position": "top",
    "pulseaudio": {
      "format": "{icon} {volume}%",
      "format-bluetooth": "{icon}  {volume}%",
      "format-bluetooth-muted": "<span font='11'>󰝟</span> {icon} {volume}%",
      "format-icons": {
        "car": "",
        "default": [
          " ",
          " ",
          "  "
        ],
        "hands-free": "󰋎",
        "headphone": "",
        "headset": "󰋎",
        "phone": "",
        "portable": ""
      },
      "format-muted": "<span font='11'>󰝟</span> {volume}%",
      "format-source": "{volume}% ",
      "format-source-muted": "",
      "on-click": "alacritty -e pulsemixer",
      "scroll-step": 5
    },
    "sway/workspaces": {
      "all-outputs": false,
      "format": "{icon}",
      "format-icons": {
{% for workspace in swaywm_workspaces %}
        "{{ workspace.id }}": "{{ workspace.name }}"{% if not loop.last %},{% endif %}

{% endfor %}
      }
    },
    "temperature": {
      "critical-threshold": 60,
      "format": "{icon} {temperatureC}°C",
      "format-icons": [
        "",
        "",
        "",
        "",
        ""
      ],
      "interval": 2,
      "tooltip": false
    },
    "tray": {
      "spacing": 10
    }
  }
]
